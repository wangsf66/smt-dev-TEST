<?xml version="1.0" encoding="UTF-8"?>
<mapping-configuration>
	<!-- (资源)发布信息, 被发布的资源都会在存储到该表中 -->
	 <table name="DEV_PUBLISHING_INFO" class="com.smt.dev.publish.resource.entity.PublishingInfo" strict="true">
		<columns>
			<column name="ID" property="id" dataType="number"  length="10" />
			<column name="RESOURCE_ID" property="resourceId" dataType="string" length="36" nullable="false"  />	
		</columns>
		
		<constraints>
			<constraint type="PRIMARY_KEY">
				<column name="ID" />
			</constraint>
		</constraints>
	</table>
</mapping-configuration>  